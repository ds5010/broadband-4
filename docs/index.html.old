<script type="module">
  // Load the Observable runtime and inspector.
  import {Runtime, Inspector} from "https://cdn.jsdelivr.net/npm/@observablehq/runtime@4/dist/runtime.js";
  
  // Your notebook, compiled as an ES module.
  import notebook from "https://api.observablehq.com/d/656c40c55d9781d3.js?v=3";
  
  const main = new Runtime().module(notebook, name => {
    if (name === "map") {
      return new Inspector(document.querySelector("#map"));
    }
    if (name === "mychart") {
      return new Inspector(document.querySelector("#mychart"));
    }
    if (name === "mywidth") {
      return new Inspector(document.querySelector("#mytest"));
    }
    if (name === "map_filter") return true;
    if (name === "myinitialize") return true;
  });

  // Redefine the cell “data” as the contents of the local file population.json;
  // this will affect the chart embedded above.
  main.redefine("data", fetch("DDI.json").then(response => response.json()));
  </script>
